<SCRIPT name="saveMapPreset(sel, i)" script="decl prstemp,prsgen,chk,t,sz,sm,cbf,x;
decl pad=stretch(0,10);
decl sc=1/map.precision[2];

decl unitsCt=1;//one map

 cbf={i,map.types[i],map.devenable[i],map.devInenable[i],map.devtype[i],map.devidx[i],map.devchan[i],   
 map.noteType[i],map.noteval[i],map.cc1[i],map.ccmode[i],
 map.ccrange[i],map.ccrangemin[i],map.ccrangemax[i],
 map.nrrange[i],map.nrshorts[i],
 map.chankeypressure[i],map.chankpval[i],
 map.chanpressureSel[i],
 map.inscale[i],
 map.fltenable[i],map.noteLenRnd[i],
 pad,
 map.nrpn[i]*sc,
 map.fltenablemin[i],map.fltenablemax[i],
 map.noteLenMin[i],map.noteLenMax[i],
 map.nrrangemin[i],map.nrrangemax[i],map.nrValPrs[i]*sc,
 map.chanpressure[i]};

prstemp=cbf;

sz=sizeof(prstemp);
sm=floor(sumof(prstemp));
prsgen={pst.version, sz, sm,unitsCt,prstemp};

if(sel==-1) return(prsgen);
else if(sel==1) pst.mapState1=prsgen;
else if(sel==2) pst.mapState2=prsgen;
else if(sel==3) pst.mapState3=prsgen;
else if(sel==4) pst.mapState4=prsgen;
else if(sel==5) pst.mapState5=prsgen;
else if(sel==6) pst.mapState6=prsgen;
else if(sel==7) pst.mapState7=prsgen;
else if(sel==8) pst.mapState8=prsgen;
else if(sel==9) pst.mapState9=prsgen;
else if(sel==10) pst.mapState10=prsgen;
else if(sel==11) pst.mapState11=prsgen;
else if(sel==12) pst.mapState12=prsgen;
else return(0);

if(df) cmm.tempV2={-8,sz,sm,unitsCt,sel,i};
mapprsReadyFlags[sel]=1;

return(sz);
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/pst/saveMapPreset" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="loadMapPreset(src, sel, i)" script="//src,sel,OFFSET
decl prstemp,t,x;
decl sc=map.precision[2];

if(src==1) prstemp=getMapPrs(sel);
else prstemp=getExtPrs(8);

if(prstemp==-1 || !sizeof(prstemp)) return(1);

decl r=0;
decl v=prstemp[r++];
decl sz=prstemp[r++];
decl chk=floor(prstemp[r++]);
decl unitsCt=prstemp[r++];
decl usz=sizeof(prstemp)-4;
decl sm=floor(sumof(subarray(prstemp,4,usz)));

if(df) cmm.tempV2={-8,v,pst.version,chk,sm,sz,usz,prstemp[3],unitsCt,usz/unitsCt,sel,i};

if( v==pst.version && sz==usz  && chk==sm && (unitsCt && unitsCt<13)) {
  r++;
  map.types[i]=prstemp[r++];
  map.devenable[i]=prstemp[r++];
  map.devInenable[i]=prstemp[r++];
  map.devtype[i]=prstemp[r++]; 
  map.devidx[i]=prstemp[r++];
  map.devchan[i]=prstemp[r++];
  map.noteType[i]=prstemp[r++];
  map.noteval[i]=prstemp[r++];
  map.cc1[i]=prstemp[r++];
  map.ccmode[i]=prstemp[r++];
  map.ccrange[i]=prstemp[r++];
  map.ccrangemin[i]=prstemp[r++];
  map.ccrangemax[i]=prstemp[r++];

  map.nrrange[i]=prstemp[r++];

  map.nrshorts[i]=prstemp[r++];
  map.chankeypressure[i]=prstemp[r++];
  map.chankpval[i]=prstemp[r++];
  map.chanpressureSel[i]=prstemp[r++];

  map.inscale[i]=prstemp[r++];
  map.fltenable[i]=prstemp[r++];
  map.noteLenRnd[i]=prstemp[r++];
  
  r+=10;

  map.nrpn[i]=prstemp[r++]*sc;
  map.fltenablemin[i]=prstemp[r++];
  map.fltenablemax[i]=prstemp[r++];
  map.noteLenMin[i]=prstemp[r++];
  map.noteLenMax[i]=prstemp[r++];

  map.nrrangemin[i]=prstemp[r++];
  map.nrrangemax[i]=prstemp[r++];
  map.nrValPrs[i]=prstemp[r++]*sc;

  map.chanpressure[i]=prstemp[r++];

} else return(sizeof(prstemp));

return(0);

" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/pst/loadMapPreset" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
</WINDOW>
