<SCRIPT name="vchg()" script="if(core.buttonNoExec) return;

decl a=firstof(x);
if(a==sizeof(x)) a=0;

mm.timepatseldur[mm.FC]=a;

decl w=131,ypos=160;
decl hx=11,h=236;
decl bsh={1,1,1,1},sz={w,w,w,w},xpos={hx,ypos,281,h},tr;
decl cz=pdnames[a],sk=6;
decl pd=4;//x padding 

if(a==1) {  bsh={0,0,0,0};}//4bar/block
if(a==2) {  bsh={0,0,0,0};}//2Bar/bl
if(a==3) { bsh={0,0,0,0};}//1bar/bl
if(a==4) { bsh={1,0,0,0}; sz={w*4+(pd*3),0,0,0}; }
if(a==5) { bsh={1,0,1,0}; sz={w*2,0,w*2,0}; }//4qtr TODO split to 4 Y panels
if(a==6) { bsh={1,0,1,0}; sz={w*2,0,w*2,0}; }//TODO split to 2 Y panels

//bsh={1,1,1,1}; sz={90,90,90,90};
setattribute(t, c, cz);
setattribute(t2, c, cz);
setattribute(t3, c, cz);
setattribute(t4, c, cz);
show(bkb1,bsh[0]); show(bkb2,bsh[1]);show(bkb3,bsh[2]); show(bkb4,bsh[3]);

setobjectrect(bkb1,{hx,ypos,sz[0],h}); 
hx+=w+pd;
setobjectrect(bkb2,{hx,ypos,sz[1],h});
hx+=w+pd;
setobjectrect(bkb3,{hx,ypos,sz[2],h});
hx+=w+pd;
setobjectrect(bkb4,{hx,ypos,sz[3],h});

core.makedurations();
//if(!ldflag) selSend2.x=a?1:0;
" trigger_script="x" trigger_type="0" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cTMt/pattRateSelS1/vchg" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
</WINDOW>
