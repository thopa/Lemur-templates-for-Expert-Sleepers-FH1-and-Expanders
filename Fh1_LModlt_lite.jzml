<SCRIPT name="update()" script="decl i,cv,ax=1,tb,tl,ts,mst,nt,sd,y;

decl lfoOn=nonnull(lfo.enable);
if(lfoOn==sizeof(lfo.enable)) return;
decl sz=sizeof(lfoOn);

for(y=0;y<sz;y++) {
 i=lfoOn[y];

 if(!freq[i] || !amt[i]) continue;

 if(delay[i]) {
    if(nxt[i]>=tms) continue; 
    nxt[i]=tms+(delay[i]*cmm.cmmtop.cMt.clfooverlay.maxlf); //next time to plot
 }

 if(type[i]==1) cv=calclfosin(i);//sine
 else if(type[i]==2) cv=calclfotri(i);//triangle
 else if(type[i]==3) cv=calclforamp(i,1);//saw
 else if(type[i]==4) cv=calclforampo(i,1);//reverse saw
 else if(type[i]==5) cv=calclfopulse(i);//Pulse
 else if(type[i]==6) cv=calclfornd(i);//rnd
 else { cv=0; }

 if(extend[i]) cv=calcCurves(i,cv);

 if(cv<.005 && startCycle[i]) { 
   startCycle[i]=0;//End of cycle/zero Crossing (not so good for Rand type) remove in-cycle flag for triggers
   accum[i]=0;
   cv=0;
 }

 output[i]=cv;
 cmm.cmmtop.cDirectInject.setDirInj(1,i,cv);
}
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/lfo/update" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="firstLoad()" script="//called by core.init at load and reset

if(!glHazChg || !mm.maxunits) maxunits=16;
else maxunits=mm.maxunits;

decl t=stretch(0,maxunits),jik=stretch(0,8);

type=stretch(1,maxunits);//{1,2,3,4,5,6,1,2};
freq=stretch(max({.05,1},0),maxunits);   //{.05,.10,.15,.25,.35,.45,.50,.65};

dur=stretch(500,maxunits);
duty=stretch(.5,maxunits);
enable={1,1,1,0,0,0,0,0};

amt=stretch(1,maxunits);

curve=stretch(8,maxunits);
thetalast=t;
thetafirst=t;
enablelast=t;

startCycle=t;

accum=thetalast;
freqlast=thetalast;

extend=t;
delay=t;
noise=t;
nxt=t;
nzmax=stretch(.2,maxunits);
nzmin=t;
output=t;
seldur=t;
randmax=stretch(1,maxunits);
randmin=t;

seldur=stretch(9,maxunits);//intervals in durations[]

" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/lfo/firstLoad" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="calclfosin(i)" script="decl cv,phase=time/1000,fchg=0,first=0,theta;
decl f=freq[i]*cmm.cmmtop.cMt.clfooverlay.maxlf;

theta = f * phase * 2 * pi;

if(f != freqlast[i])  { //thanks Andy!
  fchg=1;
  if(!freqlast[i]) thetalast[i]=theta;//init
  freqlast[i]=f; 
  accum[i] += (thetalast[i] - theta);
}

thetalast[i] = theta; 

cv=amt[i] * 0.5 * sin(theta + accum[i]) + 0.5
;

cv+=addnoise(i);

return(cv);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/lfo/calclfosin" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="calclfotri(i)" script="decl cv,phase=time/1000,theta;
decl f=freq[i]*cmm.cmmtop.cMt.clfooverlay.maxlf;

theta = f * phase * 2 * pi;

if(f != freqlast[i])  {//thanks Andy!
  if(!freqlast[i]) thetalast[i]=theta;//init
  freqlast[i]=f; 
  accum[i] += (thetalast[i] - theta);
}

thetalast[i] = theta; 

cv = amt[i] * 0.5 * asin(sin(theta + accum[i]))/1.568 + 0.5
;
cv+=addnoise(i);
return(cv);
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/lfo/calclfotri" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="addnoise(i)" script="decl y=0,t=cmm.cmmtop.cMt.clfooverlay.nzslmax;
if (noise[i]) {
   if(nzmax[i]) { 
      y=range(rand(),nzmin[i]*t,nzmax[i]*t);
      y/=100;
    } else y=rand();
}
return(y);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/lfo/addnoise" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="calclforamp(i, typ)" script="decl cv,phase=time/1000,fq=freq[i]*8;

if(!freqlast[i]) accum[i]=0;
freqlast[i]=fq;
accum[i] += fq / stfr;
accum[i] = fmod(accum[i]);

cv=accum[i]*amt[i];
cv+=addnoise(i);

return(cv);
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/lfo/calclforamp" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="calcCurves(i, cv)" script="decl mout=cv,afreq=.5,fpoles=4;
//various testing 'filter' calcs
if (extend[i]==1) cv=(1/sqrt(1+pow(mout,16))-(.5*acos(mout)) ); 
else if (extend[i]==2) cv=sqrt( 1/(1+( pow(mout,afreq*fpoles) ) ));
else if (extend[i]==3) cv=sqrt( 1/(1+( pow(mout,.2*2) ) ));
else if (extend[i]==4) cv=sqrt(mout);
else if (extend[i]==5) cv=(atan(mout)-log(mout))/4;
else if (extend[i]==6) cv=(sqrt(mout))*(2*(mout)*(pi/2)/pi);
else if (extend[i]==7) cv=acos(mout);
else if (extend[i]==8) cv=pi*mout-(pi/2);

return(cv);
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/lfo/calcCurves" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="calclforampo(i, typ)" script="decl cv,phase=time/1000,fq=freq[i]*8;
if(!freqlast[i]) accum[i]=1;//reverse ramp
freqlast[i]=fq;
accum[i] -= fq / stfr;
accum[i] = fmod(accum[i]);

cv=accum[i]*amt[i];
cv+=addnoise(i);

return(cv);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/lfo/calclforampo" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="calclfopulse(i)" script="decl cv,phase=time/1000,fq=freq[i]*5;
//decl fq=freq[i]/100;

if(!freqlast[i]) accum[i]=0;
freqlast[i]=fq;
accum[i] += fq / stfr; //(core.fps);

accum[i] = fmod(accum[i]);

if(accum[i]>(lfo.duty[i])) cv=amt[i];
//if(accum[i]>(lfo.duty[i]*cmm.cmmtop.cMt.mm.clfooverlay.slmax)) cv=amt[i];
else cv=0;

return(cv);
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/lfo/calclfopulse" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="calclfornd(i)" script="return(range(rand(),randmin[i],randmax[i] ));" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/lfo/calclfornd" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
