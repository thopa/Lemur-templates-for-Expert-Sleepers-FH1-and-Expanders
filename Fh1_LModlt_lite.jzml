<SCRIPT name="ntx(mp, n, v)" script="decl ofc=ochan | 0xb0;

txnr(mp, otarg, {ofc, 0x63, (n >> 7) & 0x7f});//ADX MSB
txnr(mp, otarg, {ofc, 0x62,  n & 0x7f});      //ADX LSB

txnr(mp, otarg, {ofc, 0x06,  (v >> 7) & 0x7f});	//VAL MSB
txnr(mp, otarg, {ofc, 0x26, v & 0x7f});		 	//VAL LSB

cFncOvr.cMapMixer.cMapMxMeters.cMapMetering(0,mp,{n,v});
//because multi value, upd map mixer metering once" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mio/ntx" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="ntxAd(mp, n, v)" script="//Advanced NTX version with last timeouts to retx Adress, INC/DEC and other
decl t=0,x=0,ofc=ochan | 0xb0;
decl amsb = (n >> 7) & 0x7f,alsb = n & 0x7f,vmsb = (v >> 7) & 0x7f,vlsb = v & 0x7f;

if(ntx_a_last != n) {
 nrpnadxmsb = amsb; 
 nrpnadxlsb = alsb;
 txnr(otarg, mp, {ofc, 0x63, amsb});
 txnr(otarg, mp, {ofc, 0x62, alsb});
 t++;
 ntxlastv=-1;
 ntx_a_last=n;
}

if(t) ntxtimeout(1);//reset timer if changes above.
 
//Save MIDI Tx bandwidth if NRPN Increment or Decrement applies
// Not all devices support this
if( !t && ntxlastv!=v) { 
 if(v>ntxlastv) { t++; x=(v-ntxlastv); }//INC
 else { x=(ntxlastv-v);}//DEC

 if(x==1) {
  if( t ) txnr(mp,otarg, {ofc, 0x60, 0x00}); //Inc 
  else txnr(mp,otarg, {ofc, 0x61, 0x00}); //Dec

  cFncOvr.cMapMixer.cMapMxMeters.cMapMetering(0,mp,{n,v});
  //because multi value, upd map mixer metering once

  ntxlastv=v;
  return;
 } else { t++; ntxlastv=-1; }//Val>1 : force full lsb+msb send.

}

if(t || ntxlastv != v) {
 txnr(mp, otarg,{ofc, 0x06, vmsb});
 txnr(mp, otarg,{ofc, 0x26, vlsb});
 ntxlastv=v;
 cFncOvr.cMapMixer.cMapMxMeters.cMapMetering(0,mp,{n,v});
}

