<SCRIPT name="calclock()" script="decl i,y,tprs,x=1920000;
if(!firstrun) {
 if(!durmax) durmax=14;
 if(!BPM) BPM=BPMdefault;
//reset durations to limit
 durations=stretch(0,durmax);
}
for(i=0;i<durmax;x=(x/2)) { // calculate note durations in ms beginning at 4 full notes
  durations[i++]=round(x/BPM); 
}

makedurations();

BPMlast=BPM;
makenewdurations=0;

lfo.masterlast=sumof(lfo.enable);

if(firstrun) mm.getnxtphrase();// song position recalc

procmon = (procmon | 0x0800) ;
" trigger_script="(makenewdurations | (BPMlast != BPM))" trigger_type="4" trigger="2" clock="0" clock_div="4" osc_message="/core/calclock" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="init()" script="if(waitframes==2) {

 tl.firstLoad();//string init 

 initWin();//set splash screen and close sub-windows

 mm.firstLoad();//init core stuff
 mio.firstLoad();
 lfo.firstLoad();
 waitframes--; 
 return;//and handle the rest on next frame
} 
else if(waitframes==1) {//2nd stage load
 cmm.firstLoad();//Matrix Control screen and all children
 cmm.firstLoadNxt();
 map.firstLoad();//init device maps and interface cmaps.topLoad() 

 arp.firstLoad();
 sttm=tms;//start time
 hrtm=0;//hour counter
 waitframes=0;
 return;
}
//and the rest loaded on 3rd frame

map.em.firstLoad();//setup map external midi preset struc
cmm.cmmtop.cRoute.cmaps.firstLoad();

buttonNoExec=0; errorLatch=0;

timersReset();

pst.firstLoad();//init preset section

cFncOvr.cMapMixer.firstrun();//init mixer

occtimer();//init occasional timer
cronitems();//do an init/firstrun of all cron jobs

sysinit=1;

procmon=0x1000;//flag init complete" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/core/init" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="makedurations()" script="mm.timedur=stretch(16,durmax);
mm.timedur[0]=durations[0];
mm.timedur[1]=durations[1];
mm.timedur[2]=durations[2];
mm.timedur[3]=durations[5]*6;//6Qtr
mm.timedur[4]=durations[3];
mm.timedur[5]=durations[5]*3;//3Qtr
mm.timedur[6]=durations[4];
mm.timedur[7]=durations[5];
mm.timedur[8]=durations[6];
mm.timedur[9]=durations[7];
mm.timedur[10]=durations[8];
mm.timedur[11]=durations[9];

//Set pattern delay available times.
mm.patdur[0]=0;//option=0 no time selected
mm.patdur[1]=durations[1]*4;//4 measure
mm.patdur[2]=durations[1]*2;//2 measure
mm.patdur[3]=durations[1];// 1 bar/16Qtr notes
mm.patdur[4]=durations[2];// 1/2=8Qtr
mm.patdur[5]=durations[3];// 4Qtr=2Seconds@120BPM
mm.patdur[6]=durations[4];// half note
mm.patdur[7]=durations[5];// qtr note
mm.patdur[8]=durations[6];// 1/8th note
mm.patdur[9]=durations[7];// 16th note
mm.patdur[10]=durations[8];// 32nd note
mm.patdur[11]=durations[9];// 64nd note

procmon = (procmon | 0x4000) ;
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/core/makedurations" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
