<SCRIPT name="enbRestoreState(x, ele)" script="//restore Gen and Mod previous selected (or default) and play states for slot enable buttons on each screen

enbCall=1;

mm.enable[mm.FC]=floor(x);

if(ldflag) return;

if(x) {//switch on GEN and MOD if previously selected
 cmmtop.swGenEn.x[0]=mm.srcUse[mm.FC];
 cmmtop.swModEn.x[0]=mm.msrcUse[mm.FC];

 if(!mm.msrcUse[mm.FC] && !mm.msrcUse[mm.FC]) {//enable but neither GEN or MOD was on, activate one in focus
   if(ele==1) cmmtop.swGenEn.x[0]=1;
   else if(ele==2)  cmmtop.swModEn.x[0]=1;
 }

} else {//switch off both GEN and MOD when enable is off
 mm.srcUse[mm.FC]=cmmtop.swGenEn.x[0];//remember last state of gen and mod
 mm.msrcUse[mm.FC]=cmmtop.swModEn.x[0];
 cmmtop.swGenEn.x[0]=0;
 cmmtop.swModEn.x[0]=0;
}
