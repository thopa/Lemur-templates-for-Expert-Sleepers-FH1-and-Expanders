<SCRIPT name="vchg()" script="decl sz=sizeof(slStepVal.x);

decl y,t,cz=i?(sz/i)/2:1;
decl ock={ stretch( max({0,1},0),cz),stretch( max({1,0},0),cz ) };
for(y=0;y<i;y++) {
 if(y) t={t,ock};
 else t=ock;
}
slStepVal.x=subarray(t,0,sizeof(t)<32?sizeof(t):32);
slStepVal.copyit();

if(++i>5) i=1;
x=0;


/*
