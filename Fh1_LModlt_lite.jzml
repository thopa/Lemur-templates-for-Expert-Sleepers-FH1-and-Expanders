 //TODO: this is a single global note buffer
 arp.lastnotebfrt[arp.bidx]=t;
 arp.lastnotebfr[arp.bidx]=LSB;//for NoteOff
 arp.lastnotemap[arp.bidx]=mp;
 arp.lastnotebfro[arp.bidx++]=i; 
}

decl oc=map.devchan[mp] & 0xF;
tx(mp,otarg,{ 0x90 | oc, LSB,vel });

midilastnote[i]=LSB;
