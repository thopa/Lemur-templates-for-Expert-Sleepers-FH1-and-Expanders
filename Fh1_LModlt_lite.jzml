<VARIABLE name="solosave=" value="-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000,-1.000000" send="0" osc_target="0" osc_trigger="1" osc_message="/cFncOvr/cMapMixer/swMapSolos/solosave" midi_target="-1" midi_trigger="1" midi_message="0x90,0x90,0,0" midi_scale="0,16383" kbmouse_target="-1" kbmouse_trigger="1" kbmouse_message="28261,25185,25964" kbmouse_scale="0,1,0,1"/>
<VARIABLE name="solobank1=" value="0.000000,0.000000,0.000000,0.000000,0.000000,0.000000,0.000000,0.000000" send="0" osc_target="0" osc_trigger="1" osc_message="/cFncOvr/cMapMixer/swMapSolos/solobank1" midi_target="-1" midi_trigger="1" midi_message="0x90,0x90,0,0" midi_scale="0,16383" kbmouse_target="-1" kbmouse_trigger="1" kbmouse_message="0,0,0" kbmouse_scale="0,1,0,1"/>
<VARIABLE name="solobank2=" value="0.000000,0.000000,0.000000,0.000000,0.000000,0.000000,0.000000,0.000000" send="0" osc_target="0" osc_trigger="1" osc_message="/cFncOvr/cMapMixer/swMapSolos/solobank2" midi_target="-1" midi_trigger="1" midi_message="0x90,0x90,0,0" midi_scale="0,16383" kbmouse_target="-1" kbmouse_trigger="1" kbmouse_message="0,0,0" kbmouse_scale="0,1,0,1"/>
<SCRIPT name="vchg()" script="// Map solos
if(ldflag || core.buttonNoExec) return;
decl ci=mxBankSel*8;
decl carr=nonnull(x - lastx);
decl chgx,t,sz=sizeof(carr);

if(carr==sizeof(x)) return;

core.buttonNoExec=1;

if(solosave[0]==-1) {//start
 solosave=map.devenable;//store device enable state
 map.devenable=stretch(0,sizeof(map.devenable));
 mm.mutes=!map.devenable;
}

//nothing selected restore from saved state
if(!sumof(x) && solosave[0]!=-1) {//revised for multi-page mixer 19aug
  map.devenable=solosave; 
  solosave=stretch(-1,sizeof(map.devenable));//clr it
  mm.mutes=!map.devenable;
}
else {
  for(t=0;t<sz;t++) {
   chgx=carr[t];
   map.devenable[ci+chgx]=x[chgx];
   mm.mutes[ci+chgx]=!x[chgx];
 }
 if(cConfigGlbs.swSoloFollow.x) mm.FC=ci+chgx;//set current slot
}
if(ci) solobank2=x;
else solobank1=x;

setMapLight(-1);
" trigger_script="x" trigger_type="0" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/swMapSolos/vchg" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="restoreOrig()" script="//restore channels if solosave is loaded
if(solosave[0]!=-1) { 
 map.devenable=solosave; 
 solosave=stretch(-1,sizeof(map.devenable));//clr it
 mm.mutes=!map.devenable;
 solobank1=stretch(0,8);
 solobank2=stretch(0,8);
}" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/swMapSolos/restoreOrig" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
