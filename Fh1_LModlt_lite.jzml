<SCRIPT name="noteoff(i)" script="// Noteoff(bufferIdx) called for each entry in arp.notebfr by arp.notesvc()
// each note in buffer can have a different output device/chan

decl n=arp.lastnotebfr[i],tm=arp.lastnotemap[i];
decl ot=map.devidx[tm],oc=map.devchan[tm];

if(n==-1 || arp.lastnotebfrt[i]<1) { arp.lastnotebfrt[i]=-1; return; }
 
tx(tm, ot,{ 0x90 | oc , n ,0});
//else oscout(otarg, '/midi/0x90',{n,0});

midilastnote[tm]=-900;" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mio/noteoff" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="notesvc(pflag)" script="//noteoff service called by core.director().   Also called by other funcs with panic flag to send all notes off
if(arp.bidx<1) return;
decl i,sz=sizeof(arp.lastnotebfr),ai=0,rord;
for(i=0;i<sz;i++){
     if(tms>arp.lastnotebfrt[i] || pflag) {
        if(arp.lastnotebfrt[i]!=-1) {
           noteoff(i);
           arp.lastnotemap[i]=-1;//devMap
           arp.lastnotebfrt[i]=-1;//time
           arp.lastnotebfro[i]=-1;//slot
           arp.lastnotebfr[i]=-1;//note
           if(arp.bidx) arp.bidx--;
           ai++;
         } 
      }
}

if(ai){//if chgs made update buffers
  ai=0;
  decl rord,oord,tord,mord; 
  for(i=0;i<sz;i++) {
    if(arp.lastnotebfr[i]!=-1) {
      rord[ai]=arp.lastnotebfr[i];
      oord[ai]=arp.lastnotebfro[i];
      mord[ai]=arp.lastnotemap[i];
      tord[ai++]=arp.lastnotebfrt[i];
    }
  }
  arp.lastnotebfr=rord;
  arp.lastnotebfro=oord;
  arp.lastnotebfrt=tord;
  arp.lastnotemap=mord;
}" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mio/notesvc" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
