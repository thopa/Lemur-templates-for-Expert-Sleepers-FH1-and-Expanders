//set switch color to green/grey
cmm.swShTheTrs.setColor(0);

cmmtop.swGenEn.x[0]=0;
cmmtop.swModEn.x[0]=0;
show(cmmtop.cGt.selSend9,0);//hide play switch, one gen

ldflag=1;
