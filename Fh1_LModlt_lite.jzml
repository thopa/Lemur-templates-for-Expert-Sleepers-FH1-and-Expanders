<SCRIPT name="topLoad()" script="//Update map screen with current
decl sz=sizeof(map.devenable);
decl i=map.current;
decl ci=mxBankSel*8;//mixer bank select 

core.buttonNoExec=1;

ldflag=1;

if(sumof(cMapMixer.swMapSolos.solobank1) || sumof(cMapMixer.swMapSolos.solobank2) ) {//solo state saved, show current solo'd meter page
 if(ci) cMapMixer.swMapSolos.x=cMapMixer.swMapSolos.solobank2;
 else cMapMixer.swMapSolos.x=cMapMixer.swMapSolos.solobank1;
} else {//clear previous slot mutes
 cMapMixer.swMapSolos.solosave=stretch(-1,sz);
 cMapMixer.swMapSolos.x=0;
}

//20Aug12
//cMapMixer.swMapSolos.x=subarray(cMapMixer.swMapSolos.solosave,ci,8);//stretch(0,8);

setMapLight(-1);//set Map enable LEDs

swSM1.x=subarray(map.devenable,ci,8);

ldflag=0;
first++;
///// Original top load:
setattribute(tStAsn1,c,map.typtxt[map.types[ci]]);
setattribute(tStAsn2,c,map.typtxt[map.types[ci+1]]);
setattribute(tStAsn3,c,map.typtxt[map.types[ci+2]]);
setattribute(tStAsn4,c,map.typtxt[map.types[ci+3]]);
setattribute(tStAsn5,c,map.typtxt[map.types[ci+4]]);
setattribute(tStAsn6,c,map.typtxt[map.types[ci+5]]);
setattribute(tStAsn7,c,map.typtxt[map.types[ci+6]]);
setattribute(tStAsn8,c,map.typtxt[map.types[ci+7]]);

setattribute(tStchn1,c,map.typnum[map.devchan[ci]]);
setattribute(tStchn2,c,map.typnum[map.devchan[ci+1]]);
setattribute(tStchn3,c,map.typnum[map.devchan[ci+2]]);
setattribute(tStchn4,c,map.typnum[map.devchan[ci+3]]);
setattribute(tStchn5,c,map.typnum[map.devchan[ci+4]]);
setattribute(tStchn6,c,map.typnum[map.devchan[ci+5]]);
setattribute(tStchn7,c,map.typnum[map.devchan[ci+6]]);
setattribute(tStchn8,c,map.typnum[map.devchan[ci+7]]);


setattribute(tStMid1,c,map.devnum[map.devidx[ci]]);
setattribute(tStMid2,c,map.devnum[map.devidx[ci+1]]);
setattribute(tStMid3,c,map.devnum[map.devidx[ci+2]]);
setattribute(tStMid4,c,map.devnum[map.devidx[ci+3]]);
setattribute(tStMid5,c,map.devnum[map.devidx[ci+4]]);
setattribute(tStMid6,c,map.devnum[map.devidx[ci+5]]);
setattribute(tStMid7,c,map.devnum[map.devidx[ci+6]]);
setattribute(tStMid8,c,map.devnum[map.devidx[ci+7]]);

updateMapNum();
updateLEDs();

cMapMixer.cMapMxMeters.clrMeters(2,0);
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/topLoad" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="updateLEDs()" script="//this now set directly at LED object:
decl ci=mxBankSel*8;
if(sizeof(map.lights)>=(ci+8)) cMapMixer.ledMapMixerStat.light=subarray(map.lights,ci,8);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/updateLEDs" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="setMapLight(i)" script="// used by cMapMixer.topLoad() to set LEDs of slot usage
decl x,mv=.7,sz;

map.lights=stretch(0,sizeof(map.lights));
map.mdout=stretch(0,sizeof(map.mdout));

//Output enable lamps
decl mapsOn=nonnull(map.devenable);
 if(mapsOn!=sizeof(map.devenable)) {
 sz=sizeof(mapsOn);
 for(x=0;x<sz;x++)  map.lights[mapsOn[x]]=mv;
}

//Input enable lamps
map.Inlights=stretch(0,sizeof(map.Inlights));
mapsOn=nonnull(map.devInenable);
map.mdin=stretch(0,sizeof(map.mdout));

if(mapsOn!=sizeof(map.devInenable)) {
 sz=sizeof(mapsOn);
 for(x=0;x<sz;x++) map.Inlights[mapsOn[x]]=mv;
}
updateLEDs();
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/setMapLight" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="getSlotAssn(i)" script="return(map.devenable[i]);
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/getSlotAssn" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="getType(x)" script="decl t,i=map.types[x];

if(i==0) return(map.noteval[x]); 
else if(i==1) return(map.cc1[x]); //*map.getMapMax(i));
else if(i==2) return(map.nrpn[x]*map.getMapMax(i));
else return(-1);

//if(t<1) t*=lprs;//scale to device precision
//  else if(t) t=lprs;//it is maxed

" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/getType" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="updateMapNum()" script="decl ci=mxBankSel*8;
setattribute(tMx1,c,tl.anum[ci+1]);
setattribute(tMx2,c,tl.anum[ci+2]);
setattribute(tMx3,c,tl.anum[ci+3]);
setattribute(tMx4,c,tl.anum[ci+4]);
setattribute(tMx5,c,tl.anum[ci+5]);
setattribute(tMx6,c,tl.anum[ci+6]);
setattribute(tMx7,c,tl.anum[ci+7]);
setattribute(tMx8,c,tl.anum[ci+8]);
//set mute buttons for current mixer page
ldflag=1;
decl i;
for(i=0;i<8;i++) swSM1.x[i]=map.devenable[ci+i];
ldflag=0;
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/updateMapNum" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="firstrun()" script="swMapSolos.solosave=stretch(-1,sizeof(map.devenable));//clr it
swMapSolos.x=0;
swMapSolos.solobank1=stretch(0,8);
swMapSolos.solobank2=stretch(0,8);
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/firstrun" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
</WINDOW>
