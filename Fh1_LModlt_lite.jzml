<SCRIPT name="meterIn(i, v)" script="return;
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/cMapMxMeters/meterIn" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="cMapMetering(s, i, v)" script="if(!map.metering) return; 
decl x=map.types[i],t=v[1];

if( (x==0 && map.noteType[i]==0) || x>2) t=v[0]; 

//if(x==2) t*=(1/127); //(1/16535);
//else t*=1/map.getMapMax(i));

meterOut(i,t*(1/map.getMapMax(i)) );" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/cMapMxMeters/cMapMetering" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="meterOut(i, v)" script="decl t=floor(i/8);
if(t != mxBankSel) return;
i%=8;
if(i) i++;
if(i<2) slDispOut1.x=v;
else if(i==2) slDispOut2.x=v;
else if(i==3) slDispOut3.x=v;
else if(i==4) slDispOut4.x=v;
else if(i==5) slDispOut5.x=v;
else if(i==6) slDispOut6.x=v;
else if(i==7) slDispOut7.x=v;
else if(i==8) slDispOut8.x=v;

map.lastMxOutTm=tms+map.meterTmOut;" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cFncOvr/cMapMixer/cMapMxMeters/meterOut" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="clrMeters(i, v)" script="if(!map.metering) return;

if(i | 2) {
slDispOut1.x=v;
slDispOut2.x=v;
slDispOut3.x=v;
slDispOut4.x=v;
slDispOut5.x=v;
slDispOut6.x=v;
