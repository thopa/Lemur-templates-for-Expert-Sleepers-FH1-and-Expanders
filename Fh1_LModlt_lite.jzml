<SCRIPT name="vchg()" script="x=floor(x);
show(csrcoff,!x);
setattribute(tGenStat,c,{'Gen OFF','Gen ON'}[x]);

if(core.buttonNoExec || ldflag) return;//topLoadCurrentMatrix calling this?

if(!x) { 
  if(mm.src[mm.FC]) mm.srcLast[mm.FC]=mm.src[mm.FC]; 
  else mm.srcLast[mm.FC]=cmm.tabdefault[0];
  mm.src[mm.FC]=0;  
}
else if(!mm.src[mm.FC]) { 
  if(mm.enbBehave && mm.srcLast[mm.FC]) mm.src[mm.FC]=mm.srcLast[mm.FC];
  else mm.src[mm.FC]=cmm.tabdefault[0];
}

if(x && !mm.enable[mm.FC] && !enbCall)  { mm.enable[mm.FC]=1; }

if(!enbCall) cmm.topLoadCurrentMatrix();
else goMyTab();
" trigger_script="x" trigger_type="0" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/swGenEn/vchg" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
