<FUNCTION name="gScPre()=(sm==1)?mm.modPre[mm.FC]:mm.srctappre[mm.FC]" send="1"/>
<SCRIPT name="upd()" script="decl i=swscModeTg.i,focus={'GEN','MOD','COMB'},scp,sp,ts;
decl hpos={42,0,214,65};
decl p1=hpos,p2=hpos;
p2[0]+=239;
decl p3=p2;
p3[0]+=244;

decl bcPre={8198,13878};
decl bcPost={22020,24166};
decl fc1,fc2,c1,c2;

if(sm>1) { fc1=focus[0]; fc2=focus[1]; c1=bcPost[0]; c2=bcPost[1]; }
else { fc1=focus[sm]; fc2=fc1; c1=bcPre[sm]; c2=bcPost[sm];}

if(i==3) { fc1=focus[2]; } //combined

setattribute(tscPreBig,c,fc1);
setattribute(tscPostBig,c,fc2);

setattribute(scGenPre,'color',c1);
setattribute(scGenOut,'color',c2);

//setattribute(tscmode,c,tl.scmodes[i]);
if(sm<2) setattribute(tstatGenSc,c,{'Off','Pre-Combine','Post','Overlayed 1','All Overlayed','Out Only'}[i]);
else setattribute(tstatGenSc,c,'Output');

setattribute(tScOut2,c,{'Output','All Overlayed'}[(i==4)?1:0]);

if(i==1) { scp=p1; sp=p2; }
else if(i<4) { scp=p1; sp=p1;}
else if(i==4) { scp=p3; sp=p3; }
else { scp=p2; sp=p2;}

setobjectrect(scGenPre,scp);
setobjectrect(scGenOut,sp);

//1nd scope pre or combined
ts=(i && i<4)?1:0;
show(scGenPre,(!i || i==2 || i==5)?0:1);
show(tstatGenSc,ts);
show(tscPreBig,ts);

//2nd scope output
ts=(i==1)?1:0;
show(scGenOut,(i && i<5)?1:0);
show(tstatGenOt,ts);
show(tscPostBig,ts);

//slot output scope
show(tSlot,mostate);
show(tinfScopeSh,!mostate);
///
show(Scmidiout,mostate);
show(tScOut2,mostate);
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cSctop/upd" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
</WINDOW>
