<SCRIPT name="firstLoad()" script="show(cDirectInject,0);

//load external device/midi map info into CC area
ipreParamSel=stretch(1,map.maxunits);//current selected index of map.em.p / name 
ipreDevIdx=stretch(1,map.maxunits);//currently sel ext device selection (ex: 1=Instrument1,2=Instrument2)
igrptype=stretch(0,map.maxunits);//function group for pre-def instruments

cmaps.cTrigs.updToggs();//keep toggles updated

cmaptab.swPredefPanel.x[0]=0;
cmaptab.swNRPredefPanel.x[0]=0;
cmaptab.cCCpredefs.swGrpExclusive.x=0;//CC
cmaptab.ccNRpredef.swGrpExclusive2.x=0;//NR
swNavPST.x[0]=0;//presets pane closed

//set predefs to first device
//CC
matchParamSel(1,3,1);
refreshDefs(1);

//NRPRN
matchParamSel(2,3,1);
refreshDefs(2);
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cRoute/cmaps/firstLoad" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="matchParamSel(typ, dir, isGrp)" script="//matchParamSel(TYPE,DIRECTION)
//match pre-defined parameter to dev map section

// check map type to see if it matches our control type for map mixer, eg.. CC==2
// and if not.. increment params until we find one or none

decl t,i=map.current;
decl sz=sizeof(map.em.ipreDevToInt),tz;

if(chkPreType(ipreParamSel[i],typ,isGrp) && !isGrp) return;

if(dir==3) ipreParamSel[i]=0;//reset parameter array pointer

if(dir==1 || dir==3) {//match forward
  //param type match to Dev Map select (CC==2/NRPN==3/and so on) 
 for(t=ipreParamSel[i];t<sz;t++) {if(chkPreType(t,typ,isGrp)) break;}
} else { //match reverse
 for(t=ipreParamSel[i]-1;t!=-1;t--) {if(chkPreType(t,typ,isGrp)) break;}
}

if(t==sz || t==-1) {//no match
 // start from beginning and get first match
 for(t=0;t<sz;t++) {if(chkPreType(t,typ,isGrp)) { ipreParamSel[i]=t; break; } }
 if(t==sz) {//still no matches set none 
