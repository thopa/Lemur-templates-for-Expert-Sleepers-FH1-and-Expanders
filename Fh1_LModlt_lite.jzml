<SCRIPT name="winPause()" script="decl i=cScreenWelcome.tWelc;

if(ldtimer>tms) { 
 if(i==2) { 
  if(cScreenWelcome.tstatCenter.light>.1 || av) {
   cScreenWelcome.tstatCenter.light-=.0075;
   cScreenWelcome.tstatCenter3.light-=.008;
   cScreenWelcome.tstatCenter2.light-=.007;
   av=1;
  } else {
   cScreenWelcome.tstatCenter.light+=.017;
   cScreenWelcome.tstatCenter3.light+=.012;
   cScreenWelcome.tstatCenter2.light+=.010;
  }
 }
 return(1);
}

if(!i) { cScreenWelcome.tWelc=1; }
if(i<2) {
  cScreenWelcome.onDone();//set second stage of welcome and timer
  cScreenWelcome.tWelc=2;
 // loadFactoryPresets();
  return(1);
} else { 
//   selectinterface(core.forder[2]);
   show(cScreenWelcome,0);
   show(cFncOvr,1);
   show(cSysMenu,1);

   //Init and show map mixer
   cFncOvr.cMapMixer.cMapMxMeters.clrMeters(3,0);
   cFncOvr.cMapMixer.topLoad();//update mixer texts
   show(cFncOvr.cMapMixer,1);

   Gllastidx=2;
   cScreenWelcome.tWelc=3;
   ldtimer=0;
   firstrun=1;

   selectinterface(core.forder[0]);
 }
return(0);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/core/winPause" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="occtimer()" script="if(!nxtocc || tms>nxtocc) {//init timer
  nxtocc=tms+occperiod;
  cronitems();
} 

if(cmm.ldflag) {
  if(ldtogg==3) { cmm.ldflag=0; ldtogg=0; }//3 frames, clear loading flag to stop autoButtons in matrix
  else ldtogg++;
}
if(buttonNoExec) buttonNoExec--;//used across a few frames to stop code selected buttons from executing ohChg()/vchg
if(cmm.enbCall) cmm.enbCall--;//Gen and Mod switch ON/OFF via Slot Enable
if(cmm.btPlayLk) cmm.btPlayLk--;//play/stop section button update
if(cmm.knAdj) cmm.knAdj=0;//used to stop knobs on multiple screens from recursing
if(routeUpd) routeUpd--;//internal routing update flag stops knob/slider recusion

if(map.metering) {
 if( tms>map.lastMxOutTm ) {//timeout clear output meters
     cFncOvr.cMapMixer.cMapMxMeters.clrMeters(2,0);
     map.lastMxOutTm=tms+map.meterTmOut;
 }

 if( tms>map.lastMxInTm ) {//timeout clear input meters
   cFncOvr.cMapMixer.cMapMxMeters.clrMeters(1,0);
   map.lastMxInTm=tms+map.meterTmOut;
 }
}


" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/core/occtimer" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
