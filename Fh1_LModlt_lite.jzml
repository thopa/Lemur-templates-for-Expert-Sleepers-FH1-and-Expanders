<SCRIPT name="theticker(i)" script="// the ticker.  Returns true if not time
//First check timing[i] && timetype[i] before calling!

if(timenxt[i]==0 || timingchange[i]) { timenxt[i]=tms; timingchange[i]=0;}
else if (timenxt[i]>tms) { procmon |= 8; return(1); }

decl x=0,t;

if(tickpatt(i)) { procmon |= 8; return(1); }

if(timefiltenable[i]) x=timetrigchk(i);//1==fail  11apr12 upd

if(!x) timeevent[i]=tms;//used by arp.arpservice and other slot

procmon |= 8;
return(x);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mm/theticker" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="matrixrun()" script="//Calculate signal for modulation matrix slots
decl i,mout=0,mout2=0,t,x,ta,f,tx,unitsOn,sz,szu,y;

unitsOn=nonnull(enable);
szu=sizeof(enable);
if(unitsOn==szu) return;
for(y=0;y<szu;y++){ 
 i=unitsOn[y];
 if(!src[i] && !msrc[i]) continue;
 if(timetype[i] && timing[i]) { if( tms<timenxt[i] && timetype[i]!=3 ) continue; }
 if(theticker(i)) continue;

 if( mutes[i] ) continue;

//MM Generator Src1
 if(src[i]) { 
  mout=srcgetval(i);
  statout[i]=mout;//keep any special negative midi note status
  srctappre[i]=clamp(mout,0,1);//scopes
  if(mout==-311) mout=0; //-311=arp and MIDI in code for no output.
  else {
   //amp
   //if mod on or no combine specified
   if( (!msrc[i] && (srcmathop[i]==-1 || srcmathop[i]==10)) || msrc[i] ) mout=clamp(mout+calcamp(i),0,1);
   //Limiting
   if(srcfltenable[i]==1) mout=range(mout,srcfltmin[i],srcfltmax[i]);
   else if(srcfltenable[i]==2) mout=clamp(mout,srcfltmin[i],srcfltmax[i]);
  }
  genOut[i]=mout;
 } else { genOut[i]=0; srctappre[i]=0; }

//MM Modifier
 if(msrc[i]) {;
  mout2=clamp(msrcgetval(i),0,1);
  modPre[i]=mout2;//scopes
  
  //Limiting
  if(msrcfltenable[i]==1) mout2=range(mout2,msrcfltmin[i],msrcfltmax[i]);
  else if(msrcfltenable[i]==2) mout2=clamp(mout2,msrcfltmin[i],msrcfltmax[i]);

  //Combine Gen+Mod
  if(src[i]) mout=mathop(srcmathop[i],mout,mout2);
  else mout=mout2;//Gen becomes signal src

  modOut[i]=mout;//scope post modOut
 } 
 else {//Only Gen src enabled
   //amp becomes combination value for mathop
   mout=mathop(srcmathop[i],mout,calcamp(i));
   modPre[i]=0;
   modOut[i]=0;
 }

 if(statout[i]==-311) { modPre[i]=0; continue;}

 //Output filter
 if(fMathOp[i] != -1) mout=mathopLst(fMathOp[i],mout,1);
 //Output Amp
 mout+=finalAmp[i];
 //Output limiting
 if(mm.outFilt[i]==1) mout=range(mout,outFiltmin[i],outFiltmax[i]);
 else if(mm.outFilt[i]==2) mout=clamp(mout,outFiltmin[i],outFiltmax[i]);

 out[i]=clamp(mout,0,1);

 if(mio.setoutputdev(i)) mio.sendmidi(i);
 cmm.cmmtop.cDirectInject.setDirInj(2,i,mout);
}
procmon = (procmon | 1) ;" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mm/matrixrun" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="getnxtphrase()" script="// update our pointer which is used to see which bar we are FC at in time
//TODO: sigs other than 4/4
if(!sumof(timing)) { 
 songplaystat=0; 
 return;
}

decl x,n,phraselen=4,seqcntbars=16,bar=durations[3];
decl npp=tms+(bar*phraselen);

songpos=tms-songstarttime;

if(songstarttime) {
  x=songpos/bar;//bars since start
  n=x%phraselen;

  songcurrentbar=n+1;
  if(songcurrentbar>=phraselen) songcurrentbar=1;

  songcurrentbarst=tms;
  songbarend=tms+bar;
  nxtbar=tms+bar;
  songcurrphrase=floor(x/phraselen);
  if( !n ) {//phrase boundry
    nxtphrase=npp;//next sysTime of phrase
    songphraseend=npp;
    //return(1);
   }
   //return(0);

} else { 
  songplaystat=1;//startSong
  songstarttime=tms; 
  songcurrentbar=1; 
  songcurrentbarst=tms;
  songbarend=tms+bar;
  songphraseend=npp;
  songcurrphrase=1;
}" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mm/getnxtphrase" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="seqintdetect(i)" script="decl x=(tms/timedurlowest[i])%1;

if(!x) return(0);//winner
else if(x<0.010) return(0);//FuzzyTime. less than 10% ok adj as needed.

return(1);

// sequence detect interval.  returns false if FC time falls on
//  an acceptable interval timedurlowest[i] chosen by user input.
//i=mod matrix slot to check time against.
// also computes next phrase time info by calling getnxtphrase();" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mm/seqintdetect" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="transport(i)" script="if(core.buttonNoExec) return;

if(i==0 || i==1) {//transport stop or initial start
  songplaystat=i;
  mm.masterswitch=i;
  songstarttime=0; 
  songcurrentbar=0; 
  songcurrentbarst=0;
  songbarend=0;
  songphraseend=0;
  nxtbar=0;
  nxtphrase=0;
  timerestnxtst[FC]=tms;
  timerestnxtend[FC]=0;
  timefiltenable[FC]=0;
  timenxt[FC]=0;
  timingchange[FC]++;
  lfo.reset();
  tpidx=stretch(0,16);//restart step gen indexes
  tpidx2=tpidx;
  tpidx3=tpidx;
  tpidx4=tpidx;
  //reset pattern LEDS
/*   decl t=stretch(0,16);
  doesnt matter
  cmm.cmmtop.cTMt.Leds.value=t;
  cmm.cmmtop.cTMt.Leds2.value=t;
  cmm.cmmtop.cTMt.Leds3.value=t;
  cmm.cmmtop.cTMt.Leds4.value=t;
*/
  srcstepcur=tpidx;
}
else if(i==2) {//pause
 songplaystat=0;
 mm.masterswitch=0;
}
else if(i==3) { //continue song
 mm.masterswitch=1;
 songplaystat=1;
 timerestnxtst[FC]=tms;
 timenxt[FC]=0;//needed for continue?
 timingchange[FC]++;
}
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mm/transport" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="tickpatt(i)" script="decl x=patdur[timepatseldur[i]],b=tpidx[i],tv;

timenxt[i]=floor(tms+x);

if(b>63) { tpidx4[i]=22; tpidx[i]=0; }
else if(b>47) { tpidx3[i]=22 ; tpidx4[i]=b%16; }
else if(b>31) { tpidx2[i]=22; tpidx3[i]=b%16;  }
else if(b>15) { tpidx2[i]=b%16; }
 
tv=tpidx[i]++;
if(i==0) return(!timepatt1[tv]);
if(i==1) return(!timepatt2[tv]);
if(i==2) return(!timepatt3[tv]);
if(i==3) return(!timepatt4[tv]);
if(i==4) return(!timepatt5[tv]);
if(i==5) return(!timepatt6[tv]);
if(i==6) return(!timepatt7[tv]);
if(i==7) return(!timepatt8[tv]);
if(i==8) return(!timepatt9[tv]);
