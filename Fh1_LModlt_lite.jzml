decl i=mm.FC;

if(ele==1) {
  cmmtop.cmmOutFilt.cLimSl2.srcLimitMin2.x=mm.outFiltmin[i];
}
else if(ele==2) {
 cmmtop.cAmpCtl.knGenAmp.x=mm.srcamt[i];
 cFncOvr.cAmpQck.knGenAmp.x=mm.srcamt[i];
}
else if(ele==3) cmmtop.cLimSl.srcLimitMin.x=mm.srcfltmin[i];
else if(ele==4) cmmtop.cLimSl.srcLimitMax.x=mm.srcfltmax[i];
else if(ele==7) cmmtop.cmmOutFilt.cLimSl2.srcLimitMax2.x=mm.outFiltmax[i];
else if(ele==8) {
 cFncOvr.cAmpQck.knModAmp.x=mm.finalAmp[i];
 cmmtop.knbFinalAmp2.x=mm.finalAmp[i];
}
else if(ele==9) cmmtop.cmmSlMat2.cLimSl2.srcLimitMin2.x=mm.msrcfltmin[i];
else if(ele==10) cmmtop.cmmSlMat2.cLimSl2.srcLimitMax2.x=mm.msrcfltmax[i];
