<SCRIPT name="srcgetval(i)" script="decl t=0;

if(src[i]==6) t=lfo.output[i];//sysLFO
else if(src[i]==7) t=getaccel(i,1);//Acceler
else if(src[i]==9) {//stepgenerator
  t=getStepVal(i,srcstepcur[i]);

  if(srcstepcur[i]>30) srcstepcur[i]=0;
  else srcstepcur[i]++;

}

t=clamp(abs(t),0,1);
if(srcinvert[i]) t=abs(1-t);
procmon = (procmon | 2) ;
return(t);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mm/srcgetval" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="srcsel(i, x)" script="// select this mm slot as active, off all others.
// i=slot x=en/disable
if(cmm.ldflag || core.buttonNoExec) return;

x=floor(x);
if(x) { 
 masterswitch=1;
 if(!cmm.cmmtop.swGenEn.x[0]) cmm.cmmtop.swGenEn.x[0]=1;//gen switch
}

src[FC]=x?i:0;

if(msrc[FC] || src[FC]) enable[FC]=1;//only toggle a matrix on state if play pressed
else enable[FC]=0;
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mm/srcsel" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="msrcsel(i, x)" script="// select this mod src mm slot as active, off all others.
// i=slot x=en/disable
if(cmm.ldflag || core.buttonNoExec) return;

x=floor(x);
if(x) { 
 masterswitch=1;
 if(!cmm.cmmtop.swModEn.x[0]) cmm.cmmtop.swModEn.x[0]=1;//mod switch
}
msrc[FC]=x?i:0;

if(msrc[FC] || src[FC]) { 
 enable[FC]=1;//Toggle a matrix on state if play pressed
 //if a combine method has not been chosen default to ADD otherwise
 // the mod is useless and bypassed with Gen signal.
  decl t=0;
  if(src[FC] && msrc[FC] && srcmathop[FC]==mm.maxmathop) {
   srcmathop[mm.FC]=1;
   t++;
  }
  cmm.cmmtop.cmmSlMat2.cGenMathOpPnl.swGenMathOp.x=0;
  cmm.cmmtop.cmmSlMat2.cGenMathOpPnl.swGenMathOp.x[t?0:mm.maxmathop]=1;//select addition or none
  cmm.cmmtop.cmmSlMat2.cGenMathOpPnl.swStageMathOp1.x=t?1:0;//open/close combine panel
}
else enable[FC]=0;

" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/mm/msrcsel" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
