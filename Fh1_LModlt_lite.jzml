<SCRIPT name="vchg()" script="decl t=bnkTyp();
if(t==-1) return;
if(t==0) {
 t=pst.loadFullMM(pstBnk);
 cmm.cmmtop.cMt.clfooverlay.topLoader();
 cmm.cmmtop.cRoute.cmaps.topLoad();
}
else if(t==1) t=pst.loadGenPreset(1,pstBnk);
else if(t==2) { 
  t=pst.loadModPreset(1,pstBnk)|pst.loadLFOPreset(1,pstBnk);
  cmm.cmmtop.cMt.clfooverlay.topLoader();
}
else if(t==3) t=pst.loadTmPreset(1,pstBnk);
else if(t==4) {
  t=pst.loadMapPreset(1,pstBnk,mm.FC); 
}
else t=0;

hasSave=t?1:0;
lastpresetErr=t;
cmm.cmmtop.cRoute.cmaps.topLoad();
cmm.topLoadCurrentMatrix();
x=0;" trigger_script="x" trigger_type="0" trigger="2" clock="0" clock_div="4" osc_message="/cFncOvr/cPresetManage/swPresetLd/vchg" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
