if(i==1) slStepVal.x={stretch( max({0,1},0),sz),stretch( max({1,n},.0),5),stretch( max({n,0},.0),sz-5)};
else if(i==2 || i==5 || i==8) slStepVal.x={stretch( max({0,1},0),sz),stretch( max({1,n},.0),5),stretch(n,sg),stretch( max({n,.010},.0),sz-sg)};
else if(i==3 || i==6 || i==9) { 
 n=.4; 
 slStepVal.x={stretch( max({0,1},.2),sz),stretch( max({1,n},.0),5),stretch(n,sg),stretch( max({n,.010},.0),sz-sg-5)};
}
else if(i==4 || i==7 || i==10) {
 decl t=sz;
 decl tv={stretch( max({0,2},.01),t*2)};
 decl sb=stretch(0,t);
 while(t--) {
   sb[t]=sqrt(tv[t]);
 }
 slStepVal.x={sb,stretch( max({1,n},.0),5),{n,n,n},stretch( max({n,.010},.0),sz-8)};
}
slStepVal.copyit();
x=0;
