    if(map.lastoutparam[mp]==p[1] && map.lastoutval[mp]==p[2]) return;
}

midiout(map.devidx[mp],p);

if(map.types[mp]==6) map.lastoutparam[mp]=((p[2] << 7) | p[1]);
else map.lastoutparam[mp]=p[1];
 
map.lastoutval[mp]=(sizeof(p)>2)?p[2]:0;

map.lastouttms[mp]=tms+nrtimeout;
mtock(mp);
//if(map.types[mp]==6) p[2]=p[3];
