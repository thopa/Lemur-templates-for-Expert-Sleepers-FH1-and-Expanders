<SCRIPT name="updatebuttonstate(seltab, x)" script="decl ele=seltab;

if(x != -1) {
 x=floor(x);
 radiostate=stretch(0,sizeof(radiostate));
 radiostate[seltab]=x;
}

core.buttonNoExec=2;//supress vchg action
lfo1Wsine.x=radiostate[0];
lfo1Wtri.x=radiostate[1];
lfo1Wsaw.x=radiostate[2];
lfo1Wrsaw.x=radiostate[3];
lfo1Wpulse.x=radiostate[4];
lfo1WRnd.x=radiostate[5];

lfo.type[ilfo]=seltab+1;
lfo.accum[ilfo]=0;
enableIt();
updateSwStat();//enable button txt

" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cMt/clfooverlay/updatebuttonstate" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="topLoader()" script="//set buttons, sliders to current LFO in focus
decl i=lfo.type[ilfo];

if(i) i-=1;

core.buttonNoExec=2;
radiostate=stretch(0,sizeof(radiostate));
radiostate[ilfo]=i;

updatebuttonstate(i,lfo.type[ilfo]);

updateKnSl(-1);

//curve/extend 
lfo.curve[ilfo]=lfo.extend[ilfo]?1:0;
setattribute(tstatLFOcurve,c,L1Log.curveSwTxt[lfo.extend[ilfo]]);

//Noise
L1Noise.x[0]=lfo.noise[ilfo];

if(core.buttonNoExec || ldflag) return;
cmm.cMMpresets.focus=4;
cmm.cMMpresets.setStageTxt();" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cMt/clfooverlay/topLoader" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="cpLFO(x)" script="decl i=ilfo,r=0;

if(x) {//copy func
clfobfr={lfo.enable[i],lfo.type[i],lfo.delay[i],
lfo.curve[i], lfo.extend[i],lfo.noise[i],lfo.nzmin[i],lfo.nzmax[i],lfo.freq[i],lfo.amt[i],lfo.duty[i],
lfo.randmin[i],lfo.randmax[i]};
}
else if ( sizeof(clfobfr) ) {//paste
 lfo.enable[i]=clfobfr[r++];
 lfo.type[i]=clfobfr[r++];
 lfo.delay[i]=clfobfr[r++];

 lfo.curve[i]=clfobfr[r++];
 lfo.extend[i]=clfobfr[r++];
 lfo.noise[i]=clfobfr[r++];
 lfo.nzmin[i]=clfobfr[r++];
 lfo.nzmax[i]=clfobfr[r++];
 lfo.freq[i]=clfobfr[r++];
 lfo.amt[i]=clfobfr[r++];
 lfo.duty[i]=clfobfr[r++];

 lfo.randmin[i]=clfobfr[r++];
 lfo.randmax[i]=clfobfr[r++];

 topLoader();
}" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cMt/clfooverlay/cpLFO" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="updateKnSl(i)" script="decl ts=(1/slmax);
decl t=(i==-1)?1:0;

if(i==1 || t) lfofreq.x=lfo.freq[ilfo];
if(i==2 || t) slNzMin.x=lfo.nzmin[ilfo];
//Random LFO min/max
if(i==3 || t) sllrandmin.x=lfo.randmin[ilfo];
//Pulse Duty
if(i==4 || t) lfoduty.x=lfo.duty[ilfo];

if(i==5 || t) lfodelay.x=lfo.delay[ilfo];
if(i==7 || t) lfo1amt2.x=lfo.amt[ilfo];
if(i==8 || t) slNzMax.x=lfo.nzmax[ilfo];
if(i==9 || t) sllrandmax.x=lfo.randmax[ilfo];


" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cMt/clfooverlay/updateKnSl" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
