<SCRIPT name="topLoadCurrentMatrix()" script="// load variables from matrix into knobs/dials/whatever here
// called every time the slot is changed or the patch is loaded from external source 
decl i=mm.FC;

//in Lite, map system and LFO follows slot
map.current=i;
cmm.cmmtop.cMt.clfooverlay.ilfo=i;

goMyTab();
ldflag=1;//signal autoSelects not to engage during load state

shOverlays(-1);
clrOverlays();

cSysMenu.topLoadSysMenu();

///set enable buttons
core.buttonNoExec=2;

// these buttons should be lit only when actually enabled not when there is a gen/mod assign
cmmtop.swGenEn.x[0]=mm.src[mm.FC]?1:0;//gen stage switch
cmmtop.swModEn.x[0]=mm.msrc[mm.FC]?1:0;//mod stage switch

mm.timing[mm.FC]=1;//timing always on in Lite

updateKnbSlMM(-1);//update knobs/sliders

cmmtop.cGenMathOpPnl.swGenMathOp.x=0;
cmmtop.cGenMathOpPnl.swGenMathOp.x[mm.srcmathop[i]]=1;  
cmmtop.cGenMathOpPnl.swStageMathOp1.x[0]=(mm.srcmathop[i]==mm.maxmathop)?0:1;//close MathOp Panel if none is selected

//Output stage filter
cmmtop.finalMathOpSel.x=0;
cmmtop.finalMathOpSel.x[mm.fMathOp[i]]=1;
cmmtop.swFinalCmbOp.x=(mm.fMathOp[i]==mm.maxfilter)?0:1;

cmmtop.srcSwDbl.x[0]=mm.srcampdbl[i];
cmmtop.srcSwInvGen.x[0]=mm.srcinvert[i];

cmmtop.cRangeSw.srcLimitEnable.x=0;
cmmtop.cRangeSw.srcLimitEnable.x[mm.srcfltenable[i]]=1;

//output filter
cmmtop.cmmOutFilt.swmmOutFlt.x=0;
cmmtop.cmmOutFilt.swmmOutFlt.x[mm.outFilt[mm.FC]]=1;
cmmtop.cmmOutFilt.cLimSl2.srcLimitMin2.x=mm.outFiltmin[mm.FC];
cmmtop.cmmOutFilt.cLimSl2.srcLimitMax2.x=mm.outFiltmax[mm.FC];

cmmtop.knbFinalAmp2.x=mm.finalAmp[mm.FC];

cmmtop.cGt.slStepVal.firstLoad();//set step values

topLoadCurrentMod();//cont loading ModSrc


