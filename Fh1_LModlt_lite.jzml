decl t=stretch(0,16);
cpRow1=t;
cpRow2=t;
cpRow3=t;
cpRow4=t;
" trigger_script="" trigger_type="5" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cTMt/firstLoad" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="pattOpt(s, op)" script="decl sz=sizeof(patt1s1.x),i,fp,sv=op;

if(s==2) {
 fp=stretch(op,sz);
} else {
 if(op==2) { sv=0; }
 else if(op==3) { sv=1; }
 else if(op==-1 || op==-2) {
   sv=(op==-1)?0:1;
   fp=wrap({sv,sv,!sv,!sv},sz);
 }

 if(op!=-1 && op!=-2) {
  fp=wrap({sv,!sv},sz);
 }
}


patt1s1.x=subarray(fp,0,16);
patt2s1.x=subarray(fp,0,16);
patt3s1.x=subarray(fp,0,16); 
patt4s1.x=subarray(fp,0,16); 

sendPatterns();

core.buttonNoExec=1;" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cTMt/pattOpt" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="loadPatterns()" script="patt1s1.loadit();
patt2s1.loadit();
patt3s1.loadit();
patt4s1.loadit();" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cTMt/loadPatterns" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="sendPatterns()" script="patt1s1.updPatt();
patt2s1.updPatt();
patt3s1.updPatt();
patt4s1.updPatt();
" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cTMt/sendPatterns" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
