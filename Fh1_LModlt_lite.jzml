<SCRIPT name="upd(s, mde)" script="decl mds={'Stage Output','Pre-Combine','Overlayed'};
decl hdsw=1;
decl bcFM={8198,22020};
decl bcFM2={24166,13878};
decl hpos={35,0,207,94};
decl hposctr={277,9,431,74};
decl outsw={790,10,180,80};
decl p1=hpos,p2=hpos;
//p2[0]+=318;
p2[1]=147;
decl p3=p2;
//p3[0]+=423;
p3[0]+=720;
p3[1]=100;
decl mrgd={292,0,423,94};

if(mde==2) { p1=mrgd; p2=p1; p3=p1; hdsw--;}

show(swscGenTg,hdsw); show(swscModTg,hdsw);//hide 1st two switches when in combined mode
//show(swscOutTg,1);

setobjectrect(scGen,p1);
setobjectrect(scMod,p2);
setobjectrect(Scmidiout,p3);

if(mde==2) { p3=hposctr;}
else p3=outsw;
//setobjectrect(swscOutTg,p3);

show(tstatGenSc,mde<2?1:0);
show(tstatModSc,mde<3?1:0);

 setattribute(scGen,'color',bcFM[fm1]);
 setattribute(scMod,'color',bcFM2[fm2]);

setattribute(tstatGenSc,c,mds[swscGenTg.i]);
if(mde<2) setattribute(tstatModSc,c,mds[swscModTg.i]);
else setattribute(tstatModSc,c,mds[2]);

//1nd scope pre or combined
show(scGen,1);
//2nd scope output
show(scMod,1);
//output scope
show(Scmidiout,1);

