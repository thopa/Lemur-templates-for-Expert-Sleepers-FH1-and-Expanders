<SCRIPT name="vchg()" script="decl t=sizeof(slStepVal.x);
decl tv={stretch( max({0,1},.01),t)};
if(i==0) {
 while(t--) {
  slStepVal.x[t]=sqrt(tv[t]);
 }
} else if(i==1) {
 while(t--) {
  slStepVal.x[t]=log(tv[t])*(1*(tv[t])-(pi/1)/pi)*.3;
 }
} else if(i==2) {
 while(t--) {
  slStepVal.x[t]=1-sqrt(tv[t]);
 }
}
slStepVal.copyit();
i++;
if(i==3) i=0;
x=0;
" trigger_script="x" trigger_type="0" trigger="2" clock="0" clock_div="4" osc_message="/cmm/cmmtop/cGt/swRampInv1/vchg" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
