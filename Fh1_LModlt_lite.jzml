ttsrcindex={0,0,0,0,0};//timing tab index

cmm.cmmtop.cTMt.firstLoad();
cmm.cmmtop.cTMt.loadPatterns();

//called after mm/arp/map init

overviewflag=0;
enbCall=0;//flag used for Mod/Gen ON/OFF switches via Slot Enable

cmm.swTopFuncMenus.x=stretch(0,sizeof(cmm.swTopFuncMenus.x));//make default MM top menu selection
cmm.swTopFuncMenus.x[0]=1;

