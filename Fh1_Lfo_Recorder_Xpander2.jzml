	ledMode.light = 1;

if (recMode == 'on')
	recMode = 'pause';
else if (playMode == 'on')
	playMode = 'pause';
else {
	
	if (recMode == 'pause')
		recMode = 'on';
	else if (playMode == 'pause')
		playMode = 'on';

	if (ledMode.light >= 0)
		ledMode.light = 0;

}" trigger_script="recCtrl.x[2]" trigger_type="0" trigger="2" clock="0" clock_div="4" osc_message="/recFaderHeavy/pauseCtrlPress" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
