<SCRIPT name="startRecord()" script="playMode = 'off';

index = 0;
timer = 0;
arrayIndex = 0;
autoIndex = 0;
recLength = automation.arrayCount * 250;
automation.clearValues();

recMode = 'on';
recMonitor.value = 0;
show(recMonitor, 1);
show(playMonitor, 0);

ledMode.light = 0;
ledMode.value = 1;" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy3/startRecord" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="stopRecord()" script="recMode = 'off';
show(recMonitor, 0);

recLength = index - 1;

ledMode.light = -1;" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy3/stopRecord" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="timing()" script="if (recMode == 'on')
	recTiming();
else if (playMode == 'on' && recLength > 0)
	playback();" trigger_script="" trigger_type="1" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy3/timing" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="recTiming()" script="if (index > 0)
	timer++;" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy3/recTiming" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="playback()" script="decl times = getexpression(automation, 'times' + (arrayIndex + 1));

if (direction == 0 ? round(timer) >= times[autoIndex] : round(timer) <= times[autoIndex]) {

	decl values = getexpression(automation, 'values' + (arrayIndex + 1));

	//AUTOMATING the recObject
	if(sumof(recObject.z) == 0)
		recObject.x = values[autoIndex];

	if (direction == 0)
		index ++;
	else
		index --;

	//Play Monitor
	playMonitor.value = (index - range[0] * recLength) / ((range[1] - range[0]) * recLength) * 100;

	automation.refresh();
}

decl endIndex;

if (direction == 0) {
	timer += speed;
	endIndex = round(range[1] * recLength);
}
else {
	timer -= speed;
	endIndex = round(range[0] * recLength);
}

arrayIndex = floor(index / 250);
autoIndex = index - arrayIndex * 250;

if (direction == 0 ? index >= endIndex : index <= endIndex) {
	
	if (direction == 0)
		index = range[0] * recLength;
	else
		index = range[1] * recLength;

	arrayIndex = floor(index / 250);
	autoIndex = index - arrayIndex * 250;
	timer = times[autoIndex];
}" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy3/playback" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="valueChange()" script="if (recMode == 'on') {

	decl values = getexpression(automation, 'values' + (arrayIndex + 1));

	//Decreases the precision of recording so more values may be added	
	decl value = round(inputX * precision) / precision;
	
	if (value != values[autoIndex - 1])
		recording(value);
}" trigger_script="inputX" trigger_type="0" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy3/valueChange" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="recCtrlPress()" script="if (recMode == 'on')
	stopRecord();
else
	startRecord();" trigger_script="recCtrl.x[0]" trigger_type="0" trigger="2" clock="0" clock_div="4" osc_message="/recFaderHeavy3/recCtrlPress" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="playCtrlPress()" script="if (playMode == 'on')
	stopPlayback();
else
	startPlayback();" trigger_script="recCtrl.x[1]" trigger_type="0" trigger="2" clock="0" clock_div="4" osc_message="/recFaderHeavy3/playCtrlPress" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="startPlayback()" script="decl times = getexpression(automation, 'times' + (arrayIndex + 1));

if (recMode == 'on' || recMode == 'pause')
	stopRecord();

if (direction == 0)
		index = range[0] * recLength;
	else
		index = range[1] * recLength;

arrayIndex = floor(index / 250);
autoIndex = index - arrayIndex * 250;
timer = times[autoIndex];

playMode = 'on';

ledMode.light = 0;
ledMode.value = 0;

show(playMonitor, 1);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy3/startPlayback" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="stopPlayback()" script="playMode = 'off';

ledMode.light = -1;

show(playMonitor, 0);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy3/stopPlayback" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="recording(input)" script="decl times = getexpression(automation, 'times' + (arrayIndex + 1)),
	values = getexpression(automation, 'values' + (arrayIndex + 1));

times[autoIndex] = timer;
values[autoIndex] = input;

setexpression(automation, 'times' + (arrayIndex + 1), times);
setexpression(automation, 'values' + (arrayIndex + 1), values);

index++;
arrayIndex = floor(index / 250);
autoIndex = index - arrayIndex * 250;

decl spacePercent = index / (automation.arrayCount * 250) * 100;

if(spacePercent == 100) {

	stopRecord();
}

automation.refresh();

recMonitor.value = spacePercent;" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy3/recording" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="pauseCtrlPress()" script="if (ledMode.light == 0)
	ledMode.light = 1;

if (recMode == 'on')
	recMode = 'pause';
else if (playMode == 'on')
	playMode = 'pause';
else {
	
	if (recMode == 'pause')
		recMode = 'on';
	else if (playMode == 'pause')
		playMode = 'on';

	if (ledMode.light >= 0)
		ledMode.light = 0;

}" trigger_script="recCtrl.x[2]" trigger_type="0" trigger="2" clock="0" clock_div="4" osc_message="/recFaderHeavy3/pauseCtrlPress" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
