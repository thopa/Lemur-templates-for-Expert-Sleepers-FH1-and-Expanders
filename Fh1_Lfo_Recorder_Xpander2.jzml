<SCRIPT name="valueLine()" script="decl i, obj = getobject(),
	rect = getobjectrect(obj),
	values, arrayIndex;
	
canvas_save(obj);
canvas_beginPath(obj);

canvas_moveTo(obj, 0, 0);

for (i = 0; i < recLength; i++) {

	arrayIndex = floor(i / 250);
	values = getexpression(automation, 'values' + (arrayIndex + 1));

	canvas_lineTo(obj, i / recLength * rect[2], rect[3] - (values[i - arrayIndex * 250] * rect[3]));

}

canvas_setStrokeStyle(obj, {1, 1, 0, 0});
canvas_stroke(obj);

canvas_refresh(obj);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy2/automation/valueLine" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
