<SCRIPT name="startPlayback()" script="decl times = getexpression(automation, 'times' + (arrayIndex + 1));

if (recMode == 'on' || recMode == 'pause')
	stopRecord();

if (direction == 0)
		index = range[0] * recLength;
	else
		index = range[1] * recLength;

arrayIndex = floor(index / 250);
autoIndex = index - arrayIndex * 250;
timer = times[autoIndex];

playMode = 'on';

ledMode.light = 0;
ledMode.value = 0;

show(playMonitor, 1);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy5/startPlayback" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="stopPlayback()" script="playMode = 'off';

ledMode.light = -1;

show(playMonitor, 0);" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy5/stopPlayback" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
<SCRIPT name="recording(input)" script="decl times = getexpression(automation, 'times' + (arrayIndex + 1)),
	values = getexpression(automation, 'values' + (arrayIndex + 1));

times[autoIndex] = timer;
values[autoIndex] = input;

setexpression(automation, 'times' + (arrayIndex + 1), times);
setexpression(automation, 'values' + (arrayIndex + 1), values);

index++;
arrayIndex = floor(index / 250);
autoIndex = index - arrayIndex * 250;

decl spacePercent = index / (automation.arrayCount * 250) * 100;

if(spacePercent == 100) {

	stopRecord();
}

automation.refresh();

recMonitor.value = spacePercent;" trigger_script="" trigger_type="4" trigger="1" clock="0" clock_div="4" osc_message="/recFaderHeavy5/recording" midi_message="0x90,0x90,0,0" midi_target="-1" flag="0"/>
